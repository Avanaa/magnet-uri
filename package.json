{
  "name": "magnet-uri",
  "version": "0.1.3",
  "description": "Parse a magnet URI and return an object of keys/values",
  "main": "index.js",
  "devDependencies": {
    "tape": "*"
  },
  "scripts": {
    "test": "tape test/*.js"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/9..latest",
      "chrome/4..latest",
      "firefox/3..latest",
      "safari/6..latest",
      "opera/15..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/4.2"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/feross/magnet-uri.git"
  },
  "keywords": [
    "magnet",
    "uri",
    "urn",
    "p2p",
    "peer-to-peer",
    "cryptolinks",
    "bittorrent",
    "webtorrent"
  ],
  "author": "Feross Aboukhadijeh <feross@feross.org> (http://feross.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feross/magnet-uri/issues"
  },
  "homepage": "https://github.com/feross/magnet-uri"
}
