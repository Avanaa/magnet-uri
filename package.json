{
  "name": "magnet-uri",
  "version": "0.1.4",
  "description": "Parse a magnet URI and return an object of keys/values",
  "main": "index.js",
  "devDependencies": {
    "tape": "2.x"
  },
  "scripts": {
    "test": "tape test/*.js"
  },
  "testling": {
    "files": "test/*.js",
